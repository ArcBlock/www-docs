{
  "name": "www-docs",
  "version": "1.2.54",
  "private": true,
  "description": "Doc site for arcblock",
  "scripts": {
    "clean": "rm -rf .cache public .blocklet",
    "start": "gatsby develop --host 0.0.0.0",
    "build": "yarn run clean && NODE_ENV=production GATSBY_APP_DOMAIN=docs.arcblock.io gatsby build",
    "serve": "gatsby serve",
    "deploy": "npm run build && an bundle --create-release && an deploy .blocklet/bundle --skip-hooks"
  },
  "files": [
    "public/",
    "blocklet.md"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ArcBlock/www-docs.git"
  },
  "keywords": [],
  "author": "wangshijun <wangshijun2010@gmail.com> (http://github.com/wangshijun)",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/ArcBlock/www-docs/issues"
  },
  "homepage": "https://github.com/ArcBlock/www-docs#readme",
  "dependencies": {
    "@arcblock/gatsby-theme-docs": "^7.28.21",
    "dotenv": "^8.0.0",
    "gatsby": "2.24.8",
    "gatsby-plugin-netlify-cache": "^1.2.0",
    "react": "^16.13.0",
    "react-dom": "^16.13.0"
  }
}
